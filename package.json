{
  "name": "redux-mount",
  "version": "0.0.1",
  "description": "Lib for faster prototyping with redux, handles storing and retreiving data for current route",
  "main": "dist/index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "./node_modules/.bin/karma start --single-run --log-level=DEBUG",
    "dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --port 8888 --inline --config webpack.config.js",
    "prod": "./node_modules/.bin/webpack -p --progress --profile --colors --config webpack.prod.config.js"
